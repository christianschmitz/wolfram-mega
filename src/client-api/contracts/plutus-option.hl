spending vesting

struct Datum {
    alice: PubKeyHash
    bob: PubKeyHash
    oracle: PubKey
}

struct Redeemer {
    msg: ByteArray
    sig: ByteArray
}

func main(datum: Datum, input: Redeemer, context: ScriptContext) -> Bool {
    tx: Tx = context.tx;

    datum.oracle.verify(input.msg, input.sig) &&
    (((input.msg == #00001) && tx.is_signed_by(datum.alice)) 
    || ((input.msg == #00002) && tx.is_signed_by(datum.bob)))
    
}